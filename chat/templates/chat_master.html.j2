{% load static %}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="{% static 'style.css' %}">
        <title>{% block title %}Page Title{% endblock %}</title>
        {% block head %}{% endblock %}
    </head>
<body>
    <nav>
        <a href="/">Home</a>
        <a href="/users/">Users</a>

        {% if "user" not in request.session %}
        <a href="/users/login/" class="rightNav">Log In</a>
        <a href="/users/signup/" class="rightNav">Sign Up</a>
        {% else %}
        <a href="/users/logout" class="rightNav">Logout</a>
        <a href="/users/dashboard/" class="rightNav">Profile</a>
        {% endif %}
    </nav>
    {% block body %}{% endblock %}
</body>
</html>